<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
   <display-name>AHoneyz Star Web Application System</display-name>

   <filter-mapping>
      <filter-name>AuthenticationFilter</filter-name>
      <url-pattern>/*</url-pattern>
   </filter-mapping>

   <servlet>
      <servlet-name>UserAdminLoginC</servlet-name>
      <servlet-class>honeyzstar.useradmin.UserAdminLoginC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminLogoutC</servlet-name>
      <servlet-class>honeyzstar.useradmin.UserAdminLogoutC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminCreateAccountC</servlet-name>
      <servlet-class>honeyzstar.useradmin.UserAdminCreateAccountC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminSuspendAccountC</servlet-name>
      <servlet-class>honeyzstar.useradmin.UserAdminSuspendAccountC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminUpdateAccountC</servlet-name>
      <servlet-class>honeyzstar.useradmin.UserAdminUpdateAccountC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminSearchAccountC</servlet-name>
      <servlet-class>honeyzstar.useradmin.UserAdminSearchAccountC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminViewAccountC</servlet-name>
      <servlet-class>honeyzstar.useradmin.UserAdminViewAccountC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminCreateRoleC</servlet-name>
      <servlet-class>honeyzstar.useradmin.UserAdminCreateRoleC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminDeleteRoleC</servlet-name>
      <servlet-class>honeyzstar.useradmin.UserAdminDeleteRoleC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminUpdateRoleC</servlet-name>
      <servlet-class>honeyzstar.useradmin.UserAdminUpdateRoleC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminSearchRoleC</servlet-name>
      <servlet-class>honeyzstar.useradmin.UserAdminSearchRoleC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminViewRoleC</servlet-name>
      <servlet-class>honeyzstar.useradmin.UserAdminViewRoleC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminGetRoleList</servlet-name>
      <servlet-class>honeyzstar.useradmin.UserAdminGetRoleList</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminGetAccountList</servlet-name>
      <servlet-class>honeyzstar.useradmin.UserAdminGetAccountList</servlet-class>
   </servlet>

   <!-- Restaurant Manager -->

   <servlet>
      <servlet-name>RestaurantManagerLoginC</servlet-name>
      <servlet-class>honeyzstar.restaurantmanager.RestaurantManagerLoginC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>RestaurantManagerLogoutC</servlet-name>
      <servlet-class>honeyzstar.restaurantmanager.RestaurantManagerLogoutC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>RestaurantManagerCreateMenuItemC</servlet-name>
      <servlet-class>honeyzstar.restaurantmanager.RestaurantManagerCreateMenuItemC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>RestaurantManagerDeleteMenuItemC</servlet-name>
      <servlet-class>honeyzstar.restaurantmanager.RestaurantManagerDeleteMenuItemC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>RestaurantManagerGetMenuItemListC</servlet-name>
      <servlet-class>honeyzstar.restaurantmanager.RestaurantManagerGetMenuItemListC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>RestaurantManagerUpdateMenuItemC</servlet-name>
      <servlet-class>honeyzstar.restaurantmanager.RestaurantManagerUpdateMenuItemC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>RestaurantManagerSearchMenuItemC</servlet-name>
      <servlet-class>honeyzstar.restaurantmanager.RestaurantManagerSearchMenuItemC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>RestaurantManagerViewMenuItemC</servlet-name>
      <servlet-class>honeyzstar.restaurantmanager.RestaurantManagerViewMenuItemC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>RestaurantManagerCreateCouponC</servlet-name>
      <servlet-class>honeyzstar.restaurantmanager.RestaurantManagerCreateCouponC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>RestaurantManagerGetCouponListC</servlet-name>
      <servlet-class>honeyzstar.restaurantmanager.RestaurantManagerGetCouponListC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>RestaurantManagerDeleteCouponC</servlet-name>
      <servlet-class>honeyzstar.restaurantmanager.RestaurantManagerDeleteCouponC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>RestaurantManagerUpdateCouponC</servlet-name>
      <servlet-class>honeyzstar.restaurantmanager.RestaurantManagerUpdateCouponC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>RestaurantManagerViewCouponC</servlet-name>
      <servlet-class>honeyzstar.restaurantmanager.RestaurantManagerViewCouponC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>RestaurantManagerSearchCouponC</servlet-name>
      <servlet-class>honeyzstar.restaurantmanager.RestaurantManagerSearchCouponC</servlet-class>
   </servlet>

   <!-- Note: All <servlet> elements MUST be grouped together and
         placed IN FRONT of the <servlet-mapping> elements -->

   <servlet-mapping>
      <servlet-name>UserAdminLoginC</servlet-name>
      <url-pattern>/loginUserAdmin</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminLogoutC</servlet-name>
      <url-pattern>/logoutUserAdmin</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminCreateAccountC</servlet-name>
      <url-pattern>/createAccount</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminSuspendAccountC</servlet-name>
      <url-pattern>/suspendAccount</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminUpdateAccountC</servlet-name>
      <url-pattern>/updateAccount</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminSearchAccountC</servlet-name>
      <url-pattern>/searchAccount</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminViewAccountC</servlet-name>
      <url-pattern>/getAccount</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminCreateRoleC</servlet-name>
      <url-pattern>/createRole</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminDeleteRoleC</servlet-name>
      <url-pattern>/deleteRole</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminUpdateRoleC</servlet-name>
      <url-pattern>/updateRole</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminSearchRoleC</servlet-name>
      <url-pattern>/searchRole</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminViewRoleC</servlet-name>
      <url-pattern>/getRole</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminGetRoleList</servlet-name>
      <url-pattern>/getRoleList</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminGetAccountList</servlet-name>
      <url-pattern>/getAccountList</url-pattern>
   </servlet-mapping>

   <!-- Restaurant Manager -->

   <servlet-mapping>
      <servlet-name>RestaurantManagerLoginC</servlet-name>
      <url-pattern>/loginRestaurantManager</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>RestaurantManagerLogoutC</servlet-name>
      <url-pattern>/logoutRestaurantManager</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>RestaurantManagerCreateMenuItemC</servlet-name>
      <url-pattern>/createMenuItem</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>RestaurantManagerDeleteMenuItemC</servlet-name>
      <url-pattern>/deleteMenuItem</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>RestaurantManagerGetMenuItemListC</servlet-name>
      <url-pattern>/getMenuItemList</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>RestaurantManagerUpdateMenuItemC</servlet-name>
      <url-pattern>/updateMenuItem</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>RestaurantManagerSearchMenuItemC</servlet-name>
      <url-pattern>/searchMenuItem</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>RestaurantManagerViewMenuItemC</servlet-name>
      <url-pattern>/getMenuItem</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>RestaurantManagerCreateCouponC</servlet-name>
      <url-pattern>/createCoupon</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>RestaurantManagerGetCouponListC</servlet-name>
      <url-pattern>/getCouponList</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>RestaurantManagerDeleteCouponC</servlet-name>
      <url-pattern>/deleteCoupon</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>RestaurantManagerUpdateCouponC</servlet-name>
      <url-pattern>/updateCoupon</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>RestaurantManagerViewCouponC</servlet-name>
      <url-pattern>/getCoupon</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>RestaurantManagerSearchCouponC</servlet-name>
      <url-pattern>/searchCoupon</url-pattern>
   </servlet-mapping>

   <filter>
      <filter-name>AuthenticationFilter</filter-name>
      <filter-class>honeyzstar.AuthenticationFilter</filter-class>
   </filter>

</web-app>