<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
      <servlet-name>UserAdminLoginC</servlet-name>
      <servlet-class>UserAdminLoginC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminLogoutC</servlet-name>
      <servlet-class>UserAdminLogoutC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminCreateAccountC</servlet-name>
      <servlet-class>UserAdminCreateAccountC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminSuspendAccountC</servlet-name>
      <servlet-class>UserAdminSuspendAccountC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminUpdateAccountC</servlet-name>
      <servlet-class>UserAdminUpdateAccountC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminSearchAccountC</servlet-name>
      <servlet-class>UserAdminSearchAccountC</servlet-class>
   </servlet>

   <servlet>
      <servlet-name>UserAdminViewAccountC</servlet-name>
      <servlet-class>UserAdminViewAccountC</servlet-class>
   </servlet>
 
   <!-- Note: All <servlet> elements MUST be grouped together and
         placed IN FRONT of the <servlet-mapping> elements -->
 
   <servlet-mapping>
      <servlet-name>UserAdminLoginC</servlet-name>
      <url-pattern>/loginUser</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminLogoutC</servlet-name>
      <url-pattern>/logoutUser</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminCreateAccountC</servlet-name>
      <url-pattern>/createAccount</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminSuspendAccountC</servlet-name>
      <url-pattern>/suspendAccount</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminUpdateAccountC</servlet-name>
      <url-pattern>/updateAccount</url-pattern>
   </servlet-mapping>
  
   <servlet-mapping>
      <servlet-name>UserAdminSearchAccountC</servlet-name>
      <url-pattern>/searchAccount</url-pattern>
   </servlet-mapping>

   <servlet-mapping>
      <servlet-name>UserAdminViewAccountC</servlet-name>
      <url-pattern>/getAccount</url-pattern>
   </servlet-mapping>
</web-app>
